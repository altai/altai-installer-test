[altai]
name=Altai Packages - $basearch
baseurl=@~CUSTOM_REPO_PATH~@
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-altai


[altai-deps]
name=Altai Deps Packages - $basearch
baseurl=@~CUSTOM_DEPS_REPO_PATH~@
failovermethod=priority
enabled=1
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-altai
file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-6
file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-6

